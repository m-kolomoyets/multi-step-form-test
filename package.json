{
    "name": "phenomenon-react-ts-template",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "engines": {
        "node": ">=22.0.0",
        "yarn": "please-use-npm",
        "npm": ">=10.9.0"
    },
    "scripts": {
        "dev": "vite --open --host",
        "build": "tsc && vite build",
        "preview": "vite preview --open --host",
        "lint:eslint": "eslint --cache --max-warnings 0 --report-unused-disable-directives ",
        "lint:stylelint": "stylelint --cache -q \"**/*.css\"",
        "lint:prettier": "prettier --check \"**/*.{js,ts,tsx,jsx,json,html,css,cjs}\"",
        "lint:prettier:fix": "prettier --write \"**/*.{js,ts,tsx,jsx,json,html,css,cjs}\"",
        "lint": "npm run lint:eslint && npm run lint:stylelint && npm run lint:prettier",
        "prepare": "husky"
    },
    "dependencies": {
        "@svg-use/react": "^0.4.3",
        "@tanstack/react-router": "^1.95.1",
        "clsx": "^2.1.1",
        "react": "^19.0.0",
        "react-dom": "^19.0.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^19.6.1",
        "@commitlint/config-conventional": "^19.6.0",
        "@eslint/compat": "^1.2.4",
        "@eslint/js": "^9.17.0",
        "@ianvs/prettier-plugin-sort-imports": "^4.4.0",
        "@svg-use/vite": "^0.1.3",
        "@tanstack/router-devtools": "^1.95.1",
        "@tanstack/router-vite-plugin": "^1.95.1",
        "@types/react": "^19.0.2",
        "@types/react-dom": "^19.0.2",
        "@typescript-eslint/eslint-plugin": "^8.19.0",
        "@typescript-eslint/parser": "^8.19.0",
        "@vitejs/plugin-react": "^4.3.4",
        "autoprefixer": "^10.4.20",
        "eslint": "^9.17.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.2.1",
        "eslint-plugin-react": "^7.37.3",
        "eslint-plugin-react-hooks": "^5.1.0",
        "globals": "^15.14.0",
        "husky": "^9.1.7",
        "postcss": "^8.4.49",
        "prettier": "^3.4.2",
        "rollup-plugin-visualizer": "^5.13.1",
        "stylelint": "^16.12.0",
        "stylelint-config-standard": "^36.0.1",
        "typescript": "^5.7.2",
        "vite": "^6.0.7",
        "vite-plugin-eslint": "^1.8.1"
    }
}
