{
    "name": "phenomenon-react-ts-template",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "engines": {
        "node": ">=22.0.0",
        "yarn": "please-use-npm",
        "npm": ">=10.9.0"
    },
    "scripts": {
        "dev:vite": "vite --open --host",
        "dev:tsc": "tsc --build --watch",
        "dev": "run-p dev:*",
        "build": "vite build",
        "preview": "vite preview --open --host",
        "lint:eslint": "eslint --cache --max-warnings 0 --report-unused-disable-directives \"**/*.{js,ts,tsx,cjs,mjs}\"",
        "lint:stylelint": "stylelint --cache -q \"**/*.css\"",
        "lint:prettier": "prettier --check \"**/*.{js,ts,tsx,json,html,css,cjs,mjs}\"",
        "lint": "run-s lint:*",
        "fix:prettier": "prettier --write \"**/*.{js,ts,tsx,json,html,css,cjs,mjs}\"",
        "prepare": "husky"
    },
    "dependencies": {
        "@formity/react": "^0.3.0",
        "@hookform/resolvers": "^3.10.0",
        "@radix-ui/react-avatar": "^1.1.2",
        "@radix-ui/react-label": "^2.1.1",
        "@radix-ui/react-radio-group": "^1.2.2",
        "@radix-ui/react-scroll-area": "^1.2.2",
        "@radix-ui/react-select": "^2.1.5",
        "@radix-ui/react-slot": "^1.1.1",
        "@react-hookz/web": "^25.0.1",
        "@svg-use/react": "^0.4.3",
        "@tanstack/react-query": "^5.64.2",
        "@tanstack/react-router": "^1.95.3",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "cmdk": "^1.0.4",
        "lucide-react": "^0.473.0",
        "prism-react-renderer": "^2.4.1",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "react-hook-form": "^7.54.2",
        "react-number-format": "^5.4.3",
        "tailwind-merge": "^2.6.0",
        "tailwindcss-animate": "^1.0.7",
        "zod": "^3.24.1"
    },
    "devDependencies": {
        "@commitlint/cli": "^19.6.1",
        "@commitlint/config-conventional": "^19.6.0",
        "@eslint/js": "^9.18.0",
        "@ianvs/prettier-plugin-sort-imports": "^4.4.0",
        "@svg-use/vite": "^0.1.3",
        "@tanstack/react-query-devtools": "^5.64.2",
        "@tanstack/router-devtools": "^1.95.3",
        "@tanstack/router-vite-plugin": "^1.95.3",
        "@types/react": "^19.0.4",
        "@types/react-dom": "^19.0.2",
        "@vitejs/plugin-react": "^4.3.4",
        "autoprefixer": "^10.4.20",
        "eslint": "^9.17.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.2.1",
        "eslint-plugin-react": "^7.37.3",
        "eslint-plugin-react-hooks": "^5.1.0",
        "globals": "^15.14.0",
        "husky": "^9.1.7",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.5.1",
        "prettier": "^3.4.2",
        "rollup-plugin-visualizer": "^5.14.0",
        "stylelint": "^16.12.0",
        "stylelint-config-standard": "^36.0.1",
        "tailwindcss": "^3.4.17",
        "typescript": "^5.7.3",
        "typescript-eslint": "^8.19.1",
        "vite": "^6.0.7",
        "vite-plugin-eslint2": "^5.0.3"
    }
}
